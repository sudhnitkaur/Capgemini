<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
<bean id="Question" class ="Question2.Question">
	<property name="questionId" value="999" />
	<property name="ques" value="What are the different cloud service providers?" />
	<property name="answers">
		<list>
			<value >Amazon Web Service (AWS)</value>
			<value >ServerSpace</value>
			<value >Microsoft Azure</value>
			<value >Google Cloud Platform</value>
		</list>
	</property>
	
	<property name="ansSet">
		<set>
			<value >Amazon Web Service (AWS)</value>
			<value >ServerSpace</value>
			<value >Microsoft Azure</value>
			<value >Google Cloud Platform</value>
		</set>
	</property>
	
	<property name="ansMap">
		<map>
			<entry key="1">
				<value >Amazon Web Service (AWS)</value>
			</entry>
			
			<entry key="2">
			<value >ServerSpace</value>
			</entry>
			
			<entry key="3">
			<value >Microsoft Azure</value>
			</entry>
			
			<entry key="4">
				<value >Google Cloud Platform</value>
			</entry>
		</map>
	</property>
	
</bean>


</beans>